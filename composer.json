{
    "name": "traderinteractive/filter-floats",
    "description": "A filtering implementation for verifying correct data and performing typical modifications to floats",
    "keywords": [
        "utility",
        "float",
        "filter"
    ],
    "license": "MIT",
    "configure": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0",
        "traderinteractive/exceptions": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TraderInteractive\\Filter\\": "src"
        }
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^1.0",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2"
    }
}
